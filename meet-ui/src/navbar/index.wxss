.mt-navbar--fixed {
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  z-index: 9999;
}

.mt-navbar__bd {
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
  flex-wrap: nowrap;
}

.mt-navbar__back,
.mt-navbar__home {
  height: 100%;
  width: 80rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 9999;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  flex-wrap: nowrap;
}

.mt-navbar__title__wp {
  flex: 1;
  height: 100%;
  text-align: center;
  font-size: 34rpx;
}

.mt-navbar--android .mt-navbar__title__wp {
  text-align: left;
}

.mt-navbar--android .mt-navbar__title--sp {
  margin-left: 45rpx;
}

.mt-navbar__title {
  display: inline-block;
  height: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
