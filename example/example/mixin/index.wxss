.cross {
  width: 60rpx;
  height: 60rpx;
  border: 1px solid transparent;
  background-color: transparent;
  position: relative;
  display: inline-block;
  border-radius: 50%;
}

.cross::after {
  content: '';
  width: 30rpx;
  height: 2rpx;
  border-radius: 2rpx;
  background-color: #333333;
  position: absolute;
  top: 50%;
  left: 50%;
  z-index: 0;
  transform: translate(-50%, -50%) rotate(45deg);
}

.cross::before {
  content: '';
  width: 2rpx;
  height: 30rpx;
  border-radius: 2rpx;
  background-color: #333333;
  position: absolute;
  top: 50%;
  left: 50%;
  z-index: 0;
  transform: translate(-50%, -50%) rotate(45deg);
}

