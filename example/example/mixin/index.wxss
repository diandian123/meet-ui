.arrows {
  position: relative;
}

.arrows::after {
  content: '';
  width: 16rpx;
  height: 16rpx;
  position: absolute;
  top: 50%;
  right: -25rpx;
  z-index: 0;
  border-width: 1px 1px 0 0;
  border-color: #aaaaaa;
  border-style: solid;
  transform: translateY(-50%) matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
}

