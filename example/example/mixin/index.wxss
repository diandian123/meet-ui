.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.ellipsisMulti {
  display: -webkit-box;
  white-space: normal;
  overflow: hidden;
  -webkit-box-orient: vertical;
  word-break: break-all;
  -webkit-line-clamp: 5;
}

.box {
  height: 360rpx;
  color: #fff;
}

.box {
  position: relative;
}

.box::after {
  content: '';
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border: 1px solid #d8d8d8;
  border-radius: 0;
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 0;
}

@media (-webkit-min-device-pixel-ratio: 2) {
  .box::after {
    width: 200%;
    height: 200%;
    border-radius: 0;
    transform: scale(0.5);
  }
}

@media (-webkit-min-device-pixel-ratio: 3) {
  .box::after {
    width: 300%;
    height: 300%;
    border-radius: 0;
    transform: scale(0.333);
  }
}

