<view class="details">
    <view class="details-header">
        <mt-cell title="{{compTitle}}">
            <mt-icon slot="left" type="custom" name="{{compIcon}}" size="64" class="component-icon"></mt-icon>
        </mt-cell>
    </view>
    <view class="details-main">
        <view class="section section--ext">
            <view class="section-hd">
                <view class="section-hd__text">示例</view>
                <text class="section-hd__icon"></text>
            </view>
            <view class="section-bd">
                <view class="pannel">
                    <view class="pannel-hd">基础使用</view>
                    <view class="pannel-bd p60">
                        <mt-button type="plain" size="m" bindtap="goPageBackTop">演示</mt-button>
                    </view>
                </view>
                <view class="pannel">
                    <view class="pannel-hd">自定义样式</view>
                    <view class="pannel-bd p60">
                        <mt-button type="plain" size="m" bindtap="goPageBackTop2">演示</mt-button>
                    </view>
                </view>
                <view class="pannel">
                    <view class="pannel-hd">标签页返回</view>
                    <view class="pannel-bd p60">
                        <mt-button type="plain" size="m" bindtap="goScrollViewbacktop">演示</mt-button>
                    </view>
                </view>
            </view>
        </view>
    </view>
</view>