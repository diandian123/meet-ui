.container {
    display: flex;
    flex-direction: column;
    height: 100vh;
}
.mod-indexlist {
    display: block;
    flex: 1;
    position: relative;
}
.mod-footer {
    padding-bottom: env(safe-area-inset-bottom);
}
.mod-filter {
    height: 100rpx;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    border-top: 1px #e9e9e9 solid;
}
.mod-filter__tips {
    font-size: 30rpx;
    color: #808080;
    text-align: right;
    margin-right: 36rpx;
    flex: 1;
}
.mod-footer__submit {
    padding: 10rpx 0 50rpx 0;
}
.mt-indexlist .mt-cell {
    padding: 16rpx 36rpx;
}
