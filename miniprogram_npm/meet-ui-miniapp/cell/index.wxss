.mt-cell{padding:30rpx 36rpx}.mt-cell--list{position:relative}.mt-cell--list::after{content:'';transform-origin:0 0;border-bottom:1px solid #e9e9e9;position:absolute;right:0;bottom:0;left:36rpx;z-index:0;transform-origin:0 center}@media (-webkit-min-device-pixel-ratio:2){.mt-cell--list::after{transform:scaleY(.5)}}@media (-webkit-min-device-pixel-ratio:3){.mt-cell--list::after{transform:scaleY(.333)}}.mt-cell--card{border-radius:16rpx;margin-bottom:24rpx}.mt-cell__content{position:relative;display:flex;flex-direction:row;align-items:center;flex-wrap:nowrap}.mt-cell__content--arrow{padding-right:30rpx}.mt-cell__content--arrow{position:relative}.mt-cell__content--arrow::after{content:'';width:16rpx;height:16rpx;position:absolute;top:50%;right:6rpx;z-index:0;border-width:1px 1px 0 0;border-color:#aaa;border-style:solid;transform:translateY(-50%) matrix(.71,.71,-.71,.71,0,0)}.mt-cell__lt{font-size:0;margin-right:18rpx}.mt-cell__lt:empty{margin-right:0}.mt-cell__icon{display:block;width:48rpx;height:48rpx}.mt-cell__ct{flex:1;overflow:hidden}.mt-cell__ct-title{display:block;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;font-size:32rpx;color:#333}.mt-cell__ct-desc{display:block;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;font-size:26rpx;color:grey;margin-top:6rpx}.mt-cell__rt{line-height:1;font-size:26rpx;color:grey;padding-left:30rpx}.mt-cell__ext{font-size:26rpx;color:grey;margin-top:20rpx}